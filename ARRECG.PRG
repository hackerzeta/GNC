CLOSE DATA

ACTIVATE WINDOW DEBUG
SUSPEND
OPEN DATA DIARIO\ADMIN
OPEN DATA LIBRES\LIBRES

SELECT 0
USE LIBRES!CTAPLAN ALIAS CTAPLAN
SELECT 0
USE ADMIN!CAJADIA ALIAS CAJADIA
SCAN
	SELECT CTAPLAN
	LOCATE FOR CODIGOVIEJO=CAJADIA.CUENTA
	IF FOUND()
		SELECT CAJADIA
		REPLACE CUENTA WITH CTAPLAN.CODIGO
	ELSE
		SELECT CAJADIA
		REPLACE CUENTA WITH 0
	ENDIF	
	SELECT CAJADIA
ENDSCAN
BROWSE
USE IN CAJADIA


