
CLOSE DATA ALL

ldfecha1 = DATE()-34
SELECT 0
USE ADMIN!IVAC ALIAS IVAC

SELECT 0
USE ADMIN!IVACOMP ALIAS IVACOMP
*SET FILTER TO FECHAIMP>=ldfecha1
GO TOP
activate window debug
suspend
SET FILTER TO FECHA>=DATE()-64
GO TOP
DO WHILE !EOF()
	lnfactura = FACTURA
    lccodigo  = CODIGO
    lningrab  = INGRAB
    SELECT IVAC
    LOCATE FOR FACTURA=lnfactura .AND. ALLTRIM(UPPER(CODIGO))==ALLTRIM(UPPER(lccodigo)) ;
           .AND. NETOGRAB = lningrab
    IF FOUND()
    	*REPLACE FECHAIMP WITH IVACOMP.FECHAIMP
    ELSE
    	IF lningrab#0
	    	WAIT WINDOW STR(LNFACTURA)
    	ENDIF
    ENDIF	
	SELECT IVACOMP
	SKIP
ENDDO	

RETURN
	