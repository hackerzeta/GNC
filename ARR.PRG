CLOSE TABLE ALL 

SELECT 0
USE dbc\COMPRAS!COMPOPRO ALIAS COMPO
set filter to codigo='00000VALTO'
GO TOP
SELECT 0
USE dbc\COMPRAS!CTA_CTE_PRO ALIAS CTA
set filter to codigo='00000VALTO'
GO TOP
SELEC COMPO
SCAN
	SELEC CTA
	LOCATE FOR FECHA=COMPO.FECHA.AND.TIPO=COMPO.TIPO.AND.FACTURA=COMPO.FACTURA.AND.CODIGO=COMPO.CODIGO
	IF !FOUND()
		SELEC COMPO
		SCATTER MEMVAR
		SELEC CTA
		APPEND BLANK
		GATHER MEMVAR
	ENDIF
	select COMPO
ENDSCAN

